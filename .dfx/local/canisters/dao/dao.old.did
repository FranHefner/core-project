type State = 
 variant {
   Adopted;
   Executed;
   Failed;
   Open;
   Rejected;
 };
type Proposal = 
 record {
   description: text;
   state: State;
   title: text;
   userPrincipal: principal;
   voteCount: int;
 };
service : {
  get_proposal: (nat) -> (opt Proposal);
  submit_proposal: (text) -> (variant {
                                Err: text;
                                Ok: opt Proposal;
                              });
}
